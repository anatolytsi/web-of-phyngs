{
  "@context": "https://www.w3.org/2019/wot/td/v1",
  "title": "WoPActuator",
  "description": "",
  "links": [
    {
      "rel": "tm:extends",
      "href": "basePhyng.model.json",
      "type": "application/td+json"
    }
  ],
  "security": "nosec_sc",
  "securityDefinitions": {
    "nosec_sc": {
      "scheme": "nosec"
    }
  },
  "properties": {
    "dimensions": {
      "description": "Phyng dimensions",
      "type": "array",
      "items": {
        "units": "m",
        "type": "number"
      },
      "minItems": 3,
      "maxItems": 3,
      "readOnly": false,
      "writeOnly": false
    },
    "rotation": {
      "description": "Phyng rotation",
      "type": "array",
      "items": {
        "units": "deg",
        "type": "number"
      },
      "minItems": 3,
      "maxItems": 3,
      "readOnly": false,
      "writeOnly": false
    },
    "custom": {
      "description": "Phyng created from custom STL",
      "type": "boolean",
      "readOnly": true,
      "writeOnly": false
    },
    "template": {
      "description": "Phyng custom template",
      "type": "string",
      "readOnly": false,
      "writeOnly": false
    }
  },
  "actions": {
    "uploadSTL": {
      "properties": {
        "file": {
          "type": "string"
        }
      }
    }
  }
}
